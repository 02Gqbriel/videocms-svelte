{
	"name": "videocms-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"docker:rm": "docker rm -f $(docker ps -a -q)",
		"docker": "docker run -d -p 127.0.0.1:3000:3000/tcp --env EncodingEnabled=true --env UploadEnabled=true --env RatelimitEnabled=false --env MaxUploadFilesize=99999999999999 --env MaxPostSize=9999999999999 kirari04/videocms:demo"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.22.3",
		"@tailwindcss/forms": "^0.5.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"tailwindcss": "^3.3.3",
		"terser": "^5.19.2",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vite-imagetools": "^4.0.19",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-image-optimizer": "^1.1.6"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/svelte-query": "^4.32.0",
		"dayjs": "^1.11.9",
		"radash": "^10.9.0",
		"svelte-french-toast": "1.0.4-beta.0"
	}
}
