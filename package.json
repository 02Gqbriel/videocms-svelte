{
	"name": "videocms-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"docker": "docker rm -f $(docker ps -a -q) && docker run -d -p 127.0.0.1:3000:3000/tcp --env RatelimitEnabled=false --env MaxUploadFilesize=99999999999999 --env MaxPostSize=9999999999999 kirari04/videocms:alpha-1 "
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.5",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.4",
		"tailwindcss": "^3.3.2",
		"terser": "^5.18.2",
		"tslib": "^2.6.0",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-imagetools": "^4.0.19",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-image-optimizer": "^1.1.6"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/svelte-query": "^4.29.19",
		"dayjs": "^1.11.8",
		"radash": "^10.9.0",
		"svelte-french-toast": "1.0.4-beta.0"
	}
}
